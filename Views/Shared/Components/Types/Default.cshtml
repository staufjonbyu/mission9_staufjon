@{ 

}
@model IEnumerable<string>
<div class="d-lg-table gap-2">

    <a asp-controller="Home" asp-action="Index" asp-route-category="" asp-route-pageNum="1">Home</a>

    @foreach(string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-category="@t" class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary" )">@t</a>
        
    }
</div>